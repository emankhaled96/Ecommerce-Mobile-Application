<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/categorybackground"
    tools:context=".Sellers.SellerProductCategoryActivity">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="75dp"
    android:background="@drawable/custom_layout">

    <TextView
        android:id="@+id/arrow_seller_cat"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:background="@drawable/ic_baseline_arrow_back_ios_24"
        android:backgroundTint="@color/pink" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/slogan_category"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="Categories"
        android:textAlignment="center"
        android:textSize="26sp"
        android:textColor="@color/pink"
android:fontFamily="@font/newbold"        />
</RelativeLayout>


    <ImageView
        android:id="@+id/female_dresses"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_marginTop="80dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pinkdress" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dresses"
        android:fontFamily="@font/newregular"
        android:textColor="@color/grey"
        android:textSize="22dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="30dp"
        />

    <ImageView
        android:id="@+id/t_shirts"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_marginTop="80dp"
android:scaleType="fitCenter"
        android:layout_marginStart="140dp"
        android:src="@drawable/pinktshirt" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shirts"
        android:fontFamily="@font/newregular"
        android:textColor="@color/grey"
        android:textSize="22dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="180dp"
        />
    <ImageView
        android:id="@+id/sweater"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:scaleType="fitCenter"
        android:layout_marginTop="80dp"
        android:layout_marginStart="280dp"
