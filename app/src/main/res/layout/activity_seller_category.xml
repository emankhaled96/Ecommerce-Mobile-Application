    <ImageView
        android:id="@+id/sports_t_shirts"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:scaleType="fitCenter"
        android:layout_marginTop="410dp"
        android:layout_marginStart="140dp"

        android:src="@drawable/sporttshirt" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sport Shirts"
        android:fontFamily="@font/newregular"
        android:textColor="@color/grey"
        android:textSize="22dp"
        android:layout_marginTop="510dp"
        android:layout_marginStart="150dp"
        />

    <ImageView
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:src="@drawable/pinkglasses"
            android:id="@+id/glasses"
        android:layout_marginTop="570dp"
        android:layout_marginStart="3dp"
        android:scaleType="fitCenter"
            />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="670dp"
        android:fontFamily="@font/newregular"
        android:text="Glasses"
        android:textColor="@color/grey"
        android:textSize="22dp" />



        <ImageView
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:src="@drawable/pinkshoes"
            android:id="@+id/shoes"
            android:scaleType="fitCenter"
            android:layout_marginTop="570dp"
            android:layout_marginStart="140dp"
            />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shoes"
        android:fontFamily="@font/newregular"
        android:textColor="@color/grey"
        android:textSize="22dp"
        android:layout_marginTop="670dp"
        android:layout_marginStart="180dp"
        />







        <ImageView
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:src="@drawable/pinklaptop"
            android:id="@+id/laptop_pc"
            android:scaleType="fitCenter"
            android:layout_marginTop="570dp"
            android:layout_marginStart="280dp"
            />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laptops"
        android:fontFamily="@font/newregular"
        android:textColor="@color/grey"
        android:textSize="22dp"
        android:layout_marginTop="670dp"
        android:layout_marginStart="305dp"
        />


</RelativeLayout>